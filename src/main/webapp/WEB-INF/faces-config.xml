<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToIndex</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToTutorLogin</from-outcome>
			<to-view-id>/views/userlogin/tutorlogin.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToTutorSignup</from-outcome>
			<to-view-id>/views/user/tutorsignup.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToLandingPage</from-outcome>
			<to-view-id>/views/dashboard.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToLoginPage</from-outcome>
			<to-view-id>/views/userlogin/tutorlogin.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>goToHomePage</from-outcome>
			<to-view-id>views/userlogin/tutorhomepage.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>


	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	<!-- <lifecycle> <phase-listener> com.banana.spytutors.web.common.UACompatibleHeaderPhaseListener 
		</phase-listener> </lifecycle> -->


	<component>
		<component-type>org.primefaces.component.AtlasMenu</component-type>
		<component-class>org.primefaces.atlas.component.menu.AtlasMenu</component-class>
	</component>

	<render-kit>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>org.primefaces.component.AtlasMenuRenderer</renderer-type>
			<renderer-class>org.primefaces.atlas.component.menu.AtlasMenuRenderer</renderer-class>
		</renderer>
	</render-kit>

</faces-config>


